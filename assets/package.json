{
  "name": "@cyclingapps/component-bundle",
  "version": "1.0.0",
  "keywords": [
    "symfony-ux"
  ],
  "symfony": {
    "controllers": {
      "modal": {
        "main": "dist/modal.js",
        "fetch": "eager",
        "enabled": true
      },
      "callout": {
        "main": "dist/callout.js",
        "enabled": true,
        "autoimport": {
          "@cyclingapps/component-bundle/dist/callout.css": true
        }
      },
      "widget": {
        "main": "dist/widget.js",
        "enabled": true,
        "autoimport": {
          "@cyclingapps/component-bundle/dist/widget.css": true
        }
      }
    },
    "importmap": {
      "@hotwired/stimulus": "^3.0.0",
      "@popperjs/core": "^2.11.8",
      "bootstrap": "^5.3.8"
    }
  },
  "peerDependencies": {
    "@hotwired/stimulus": "^3.0.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.8"
  }
}
